  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "vue/multi-word-component-names": [
    "error",
    {
      "ignores": [
        "Tree"
      ]
    }
  ]
